thor, affiliation in zip(authors, affiliations):\n","        if re.search(r\"\\b(pharmaceutical|biotech|company)\\b\", affiliation, re.IGNORECASE):\n","            non_academic_authors.append(author)\n","    return non_academic_authors\n","\n","def extract_relevant_info(paper_details):\n","    non_academic_authors = identify_non_academic_authors(paper_details[\"authors\"], paper_details[\"affiliations\"])\n","    return {\n","        \"pubmed_id\": paper_details[\"pubmed_id\"],\n","        \"title\": paper_details[\"title\"],\n","        \"publication_date\": paper_details[\"publication_date\"],\n","        \"non_academic_authors\": non_academic_authors,\n","        \"company_affiliations\": [affiliation for affiliation in paper_details[\"affiliations\"] if re.search(r\"\\b(pharmaceutical|biotech|company)\\b\", affiliation, re.IGNORECASE)],\n","        \"corresponding_author_email\": \"\"  # Not available in PubMed data\n","    }\n","\n","def main(query): # Modified to accept query as an argument\n","    logging.info(\"Fetching papers from PubMed...\")\n","    paper_ids = fetch_papers(query)\n","\n","    logging.info(\"Processing papers...\")\n","    papers = []\n","    for paper_id in paper_ids:\n","        paper_details = get_paper_details(paper_id)\n","        relevant_info = extract_relevant_info(paper_details)\n","        papers.append(relevant_info)\n","\n","    logging.info(\"Printing results to console...\")\n","    for paper in papers:\n","        print(paper)\n","\n","# Call main with your desired query\n","if __name__ == \"__main__\":\n","    # Example query\n","    query = \"cancer research\"\n","    main(query) # Pass the query to the main function"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"bPwRXOOuJEbo","executionInfo":{"status":"ok","timestamp":1741762642365,"user_tz":-330,"elapsed":8073,"user":{"displayName":"Mayuri Terala","userId":"15638181809746255573"}},"outputId":"b85937a0-9f9a-48ac-a649-e52d7b67d2a3"},"execution_count":2